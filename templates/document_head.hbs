<!-- Service Status -->
<script src="https://cdn.statuspage.io/se-v2.js"></script>
<script>
  var sp = new StatusPage.page({ page: 'xhh3t32ngbpr' });
  sp.status(
    {
      success: statusPageCallback
    });

  function statusPageCallback(data) {
    var status = document.getElementById('service-status');
    status.classList.add('c-service-status-icon--' + data.status.indicator);
  }
</script>

